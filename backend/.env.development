# .env.development
NODE_ENV=development
PORT=5000

# MongoDB connection string for development
MONGO_URI=mongodb://mongo:27017/campusconnect_dev
MONGO_URI_TEST=mongodb://mongo:27017/campusconnect_test

# Redis configuration 
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=redispassword

# Session configuration
SESSION_SECRET=yourDevelopmentSessionSecret
SESSION_LIFETIME=86400000   # 1 day in ms

# Cookie options - In development
COOKIE_SECURE=false
COOKIE_SAME_SITE=Lax
